<!DOCTYPE html>
<html lang="ja">
<head>
<meta name="content-language" content="ja">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">

<!-- meta infomation -->
<title>{{ BaseInfo.shop_name }}{% if subtitle is defined and subtitle is not empty %} / {{ subtitle }}{% elseif title is defined and title is not empty %} / {{ title }}{% endif %}</title>
{% if PageLayout.author is not empty %}
    <meta name="author" content="{{ PageLayout.author }}">
{% endif %}
{% if PageLayout.description is not empty %}
    <meta name="description" content="{{ PageLayout.description }}">
{% endif %}
{% if PageLayout.keyword is not empty %}
    <meta name="keywords" content="{{ PageLayout.keyword }}">
{% endif %}
{% if PageLayout.meta_robots is not empty %}
    <meta name="robots" content="{{ PageLayout.meta_robots }}">
{% endif %}

<!-- include style sheet -->
<link rel="stylesheet" href="{{ app.config.front_urlpath }}/css/bootstrap.min.css?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/css" />
<link rel="stylesheet" href="{{ app.config.front_urlpath }}/css/font-awesome.min.css?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/css" />
<link rel="stylesheet" href="{{ app.config.front_urlpath }}/css/slidebars.min.css?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/css" />
<link rel="stylesheet" href="{{ app.config.front_urlpath }}/css/style.css?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/css" />

<!--[if lt IE 9]>
  <script type="text/javascript" src="{{ app.config.front_urlpath }}/js/html5shiv.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>
  <script type="text/javascript" src="{{ app.config.front_urlpath }}/js/respond.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>
<![endif]-->

{% block stylesheet %}{% endblock %}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ app.config.front_urlpath }}/js/vendor/jquery-1.11.3.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"><\/script>')</script>
</head>

<body id="page_{{ app.request.get('_route') }}" class="{{ body_class|default('other_page') }}">

<!-- contents start -->
<div id="sb-site" canvas="container">

<!-- header start -->
{# ▼HeaderInternal COLUMN #}
{% if PageLayout.Header %}
    {# ▼上ナビ #}
    {{ include('block.twig', {'Blocks': PageLayout.Header}) }}
    {# ▲上ナビ #}
{% endif %}
{# ▲HeaderInternal COLUMN #}


{% block eyecatch %}{% endblock %}

{% block breadcrumb %}{% endblock %}  


<div class="container">
  <div class="row">

    <!-- main-contents start -->
    <div class="l-main-contents col-sm-9">
      {% block main %}{% endblock %}  
    </div>
    <!-- /main-contents end -->

    <!-- nav start -->
    {# ▼RIGHT COLUMN #}
    {% if PageLayout.SideRight %}
      {# ▼右ナビ #}
        {{ include('block.twig', {'Blocks': PageLayout.SideRight}) }}
      {# ▲右ナビ #}
    {% endif %}
    {# ▲RIGHT COLUMN #}
    <!-- /nav end -->
  
  </div>
</div>

<!-- footer start -->
{# ▼Footer COLUMN#}
{% if PageLayout.Footer %}
    {# ▼上ナビ #}
    {{ include('block.twig', {'Blocks': PageLayout.Footer}) }}
    {# ▲上ナビ #}
{% endif %}
{# ▲Footer COLUMN#}
<!-- /footer end -->

</div>
<div class="sp-menu-cover js-close-any"></div>
<!-- /contents end -->

<!-- spMenu -->
<div class="sb-slidebar side-menu" off-canvas="sp-menu-body right overlay">
  <div class="sp-menu-close-button-area js-close-any"><i class="fa fa-times" aria-hidden="true"></i></div>
  <ul>
    <li><a href="http://153.121.44.230/staging/printsendhtml/">トップページ</a></li>
    <li><a href="http://153.121.44.230/staging/printsendhtml/readme.html">初めての方</a></li>
    <li><a href="http://153.121.44.230/staging/printsendhtml/guide/price/index.html">料金について</a></li>
    <li><a href="http://153.121.44.230/staging/printsendhtml/">納期について</a></li>
    <li><a href="http://153.121.44.230/staging/printsendhtml/">よくある質問</a></li>
    <li><a href="http://153.121.44.230/staging/printsendhtml/regist/kiyaku/">お問い合わせ</a></li>
  </ul>
  <p class="side-menu-contacttitle"><img class="phone-icon" src="{{ app.config.front_urlpath }}/img/common/icon_telephone.png" alt="電話">お問い合わせ電話番号 /</p>
  <p class="side-menu-phone-number">03-5918-8046</p>
  <p class="side-menu-time">受付時間：10：00～18：00</p>
  <p class="side-menu-holiday">定休日：土日祝</p>
</div>
<!-- /spMenu -->


<!-- include script --> 
<script src="{{ app.config.front_urlpath }}/js/jquery-1.12.0.min.js" type="text/javascript"></script> 
<script src="{{ app.config.front_urlpath }}/js/bootstrap.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/javascript"></script> 
<script src="{{ app.config.front_urlpath }}/js/slidebars.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/javascript"></script>
<script src="{{ app.config.front_urlpath }}/js/accordion.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/javascript"></script>
<script src="{{ app.config.front_urlpath }}/js/common.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}" type="text/javascript"></script>
<script src="{{ app.config.front_urlpath }}/js/function.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>
<script src="{{ app.config.front_urlpath }}/js/eccube.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>
{% block javascript %}{% endblock %}
</body>
</html>
